{
  "name": "beer",
  "version": "1.0.0",
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node test && ./node_modules/mocha/bin/mocha server/**/*.test.js",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "test:*,app:*"
      }
    },
    "coverage": {
      "command": "node test && ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter lcovonly -R spec server/**/*.test.js",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "test:*,app:*"
      }
    }
  },
  "dependencies": {
    "async": "^2.1.4",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bcrypt": "^1.0.2",
    "better-npm-run": "^0.0.13",
    "bluebird": "^3.5.0",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "connected-react-router": "^4.0.0-beta.4",
    "debug": "^2.6.1",
    "express": "^4.14.0",
    "fast-json-patch": "^1.1.6",
    "file-loader": "^0.9.0",
    "fs-extra": "^2.0.0",
    "google-map-react": "^0.22.0",
    "has": "^1.0.1",
    "history": "^4.4.1",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.4",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "mongodb": "^2.2.22",
    "passport": "^0.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.6",
    "react-router-addons-controlled": "^0.0.1",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "reconnecting-websocket": "^3.0.3",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-form-material-ui": "^4.1.2",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.1.1",
    "store": "^2.0.4",
    "styled-components": "^1.1.1",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-polyfill": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cheerio": "^0.20.0",
    "codecov": "^2.1.0",
    "dockerode": "^2.3.2",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.10.0",
